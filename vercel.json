{
  "installCommand": "cd frontend-web && npm install",
  "buildCommand": "cd frontend-web && npm run build",
  "outputDirectory": "frontend-web/dist",
  "framework": "vite",
  "rewrites": [
    { 
      "source": "/api/(.*)", 
      "destination": "https://talentos-production-35e8.up.railway.app/$1" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/index.html" 
    }
  ]
}
